{
  "name": "real_test",
  "author": "lucasvieira.dev@gmail.com",
  "version": "0.2.1",
  "description": "Package that provides real e2e tests",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "teste2e",
    "e2e",
    "nightwatch",
    "test"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive test",
    "selenium": "java -jar webdrivers/selenium-server-standalone-2.49.1.jar",
    "start": "babel-node code/index.js && nightwatch",
    "build": "babel code --out-dir bin",
    "nightwatch": "nightwatch",
    "release:patch": "npm test && release-it patch",
    "release:minor": "npm test && release-it minor",
    "release:major": "npm test && release-it major"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chromedriver": "^2.19.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "glob": "^6.0.4",
    "nightwatch": "^0.8.6",
    "ramda": "0.19.1",
    "read-file": "^0.2.0",
    "shelljs": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "mocha": "^2.3.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "bin": {
    "real-test": "bin/index.js"
  }
}
